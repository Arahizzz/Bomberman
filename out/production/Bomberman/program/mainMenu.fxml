<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
          fx:controller="program.MainMenuController">
   <columnConstraints>
      <ColumnConstraints/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints/>
   </rowConstraints>
   <children>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="150.0" prefWidth="400.0">
         <children>
            <Button fx:id="single" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                    text="Single player">
               <VBox.margin>
                  <Insets bottom="10.0"/>
               </VBox.margin>
            </Button>
            <Button fx:id="multi" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                    text="Multiplayer">
               <VBox.margin>
                  <Insets bottom="10.0"/>
               </VBox.margin>
            </Button>
            <Button fx:id="exit" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                    text="Exit">
               <VBox.margin>
                  <Insets bottom="10.0"/>
               </VBox.margin>
            </Button>
         </children>
         <GridPane.margin>
            <Insets bottom="20.0" left="10.0" right="10.0" top="20.0"/>
         </GridPane.margin>
      </VBox>
   </children>
</GridPane>
